{
  "name": "@conradklek/webs",
  "author": "Conrad Klek",
  "version": "0.1.32",
  "type": "module",
  "description": "A Javascript framework",
  "bin": {
    "webs": "build.js"
  },
  "files": [
    "lib",
    "build.js",
    "compiler.js",
    "index.js",
    "parser.js",
    "plugin.js",
    "reactivity.js",
    "renderer.js",
    "runtime.js",
    "server.js",
    "cache.js"
  ],
  "exports": {
    ".": "./index.js",
    "./server": "./server.js",
    "./compiler": "./lib/compiler.js",
    "./plugin": "./plugin.js"
  },
  "scripts": {
    "dev": "NODE_ENV=development bun run ./build.js",
    "dev:demo": "cd ../webs-site && NODE_ENV=development bun run ../webs-framework/build.js",
    "start": "NODE_ENV=production bun run ./build.js",
    "start:demo": "cd ../webs-site && NODE_ENV=production bun run ../webs-framework/build.js",
    "format": "prettier --write ."
  },
  "dependencies": {},
  "peerDependencies": {
    "tailwindcss": "^4.1.12",
    "bun-plugin-tailwind": "^0.0.15"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
