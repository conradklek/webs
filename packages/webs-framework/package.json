{
  "name": "@conradklek/webs",
  "author": "Conrad Klek",
  "version": "0.1.31",
  "type": "module",
  "description": "A Javascript framework",
  "bin": {
    "webs": "bin/cli.js"
  },
  "files": [
    "bin",
    "dist",
    "client",
    "server",
    "lib",
    "index.js",
    "plugin.js"
  ],
  "exports": {
    ".": "./index.js",
    "./client": "./client/index.js",
    "./server": "./server/request-handler.js",
    "./compiler": "./lib/compiler.js",
    "./plugin": "./plugin.js"
  },
  "scripts": {
    "dev": "NODE_ENV=development bun run ./bin/cli.js",
    "dev:demo": "cd ../webs-site && NODE_ENV=development bun run ../webs-framework/bin/cli.js",
    "start": "NODE_ENV=production bun run ./bin/cli.js",
    "start:demo": "cd ../webs-site && NODE_ENV=production bun run ../webs-framework/bin/cli.js",
    "format": "prettier --write ."
  },
  "dependencies": {},
  "peerDependencies": {
    "tailwindcss": "^4.1.12",
    "bun-plugin-tailwind": "^0.0.15"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
